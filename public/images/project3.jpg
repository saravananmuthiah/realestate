<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="sky" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FFF8DC;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="bungalow" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#DEB887;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#F4A460;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#D2691E;stop-opacity:1" />
    </linearGradient>
  </defs>

  <!-- Sky background -->
  <rect width="400" height="300" fill="url(#sky)"/>

  <!-- Ground -->
  <rect x="0" y="220" width="400" height="80" fill="#90EE90"/>

  <!-- Main bungalow structure -->
  <rect x="100" y="140" width="200" height="80" fill="url(#bungalow)" stroke="#8B4513" stroke-width="2"/>

  <!-- Traditional sloping roof -->
  <polygon points="90,140 200,100 310,140" fill="#8B0000" stroke="#654321" stroke-width="2"/>
  <polygon points="95,145 200,110 305,145" fill="#DC143C" stroke="#8B0000" stroke-width="1"/>

  <!-- Windows -->
  <rect x="120" y="155" width="25" height="20" fill="#87CEEB" stroke="#654321" stroke-width="1"/>
  <rect x="155" y="155" width="25" height="20" fill="#87CEEB" stroke="#654321" stroke-width="1"/>
  <rect x="220" y="155" width="25" height="20" fill="#87CEEB" stroke="#654321" stroke-width="1"/>
  <rect x="255" y="155" width="25" height="20" fill="#87CEEB" stroke="#654321" stroke-width="1"/>

  <!-- Door -->
  <rect x="190" y="165" width="20" height="55" fill="#8B4513" stroke="#654321" stroke-width="2"/>
  <circle cx="195" cy="185" r="2" fill="#FFD700"/>

  <!-- Veranda -->
  <rect x="85" y="180" width="230" height="10" fill="#D2691E" stroke="#8B4513" stroke-width="1"/>
  <rect x="80" y="180" width="10" height="40" fill="#D2691E" stroke="#8B4513" stroke-width="1"/>
  <rect x="310" y="180" width="10" height="40" fill="#D2691E" stroke="#8B4513" stroke-width="1"/>

  <!-- Pillars -->
  <rect x="90" y="190" width="6" height="30" fill="#F5DEB3" stroke="#D2691E" stroke-width="1"/>
  <rect x="304" y="190" width="6" height="30" fill="#F5DEB3" stroke="#D2691E" stroke-width="1"/>
  <rect x="150" y="190" width="6" height="30" fill="#F5DEB3" stroke="#D2691E" stroke-width="1"/>
  <rect x="244" y="190" width="6" height="30" fill="#F5DEB3" stroke="#D2691E" stroke-width="1"/>

  <!-- Garden elements -->
  <circle cx="50" cy="250" r="15" fill="#228B22"/>
  <circle cx="350" cy="250" r="15" fill="#228B22"/>
  <circle cx="70" cy="240" r="12" fill="#32CD32"/>
  <circle cx="330" cy="240" r="12" fill="#32CD32"/>

  <!-- Traditional Indian elements -->
  <circle cx="200" cy="115" r="6" fill="#FFD700" stroke="#FFA500" stroke-width="2"/>
  <path d="M190 115 Q200 105 210 115" fill="none" stroke="#FFA500" stroke-width="2"/>

  <!-- Side extension -->
  <rect x="70" y="160" width="30" height="40" fill="url(#bungalow)" stroke="#8B4513" stroke-width="1"/>
  <polygon points="60,160 100,140 100,160" fill="#8B0000" stroke="#654321" stroke-width="1"/>
  <rect x="300" y="160" width="30" height="40" fill="url(#bungalow)" stroke="#8B4513" stroke-width="1"/>
  <polygon points="300,160 330,140 330,160" fill="#8B0000" stroke="#654321" stroke-width="1"/>

  <!-- Pathway -->
  <rect x="180" y="220" width="40" height="60" fill="#D2B48C" stroke="#8B4513" stroke-width="1"/>
  <rect x="190" y="240" width="20" height="40" fill="#F5DEB3" stroke="#D2691E" stroke-width="1"/>

  <!-- Title -->
  <text x="200" y="20" text-anchor="middle" fill="#8B4513" font-family="serif" font-size="16" font-weight="bold">Traditional Indian Bungalow</text>
</svg>
