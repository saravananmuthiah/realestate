<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="sky" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FFFACD;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="duplex" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FFF8DC;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#F5DEB3;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#DEB887;stop-opacity:1" />
    </linearGradient>
  </defs>

  <!-- Sky background -->
  <rect width="400" height="300" fill="url(#sky)"/>

  <!-- Ground -->
  <rect x="0" y="220" width="400" height="80" fill="#90EE90"/>

  <!-- Main duplex structure -->
  <rect x="100" y="120" width="200" height="100" fill="url(#duplex)" stroke="#D2691E" stroke-width="2"/>

  <!-- Floor divider -->
  <line x1="100" y1="170" x2="300" y2="170" stroke="#8B4513" stroke-width="2"/>
  <line x1="100" y1="172" x2="300" y2="172" stroke="#654321" stroke-width="1"/>

  <!-- Duplex roof -->
  <polygon points="90,120 200,80 310,120" fill="#8B0000" stroke="#654321" stroke-width="2"/>
  <polygon points="95,125 200,90 305,125" fill="#DC143C" stroke="#8B0000" stroke-width="1"/>

  <!-- Upper floor windows -->
  <rect x="120" y="130" width="25" height="20" fill="#87CEEB" stroke="#4682B4" stroke-width="1"/>
  <rect x="155" y="130" width="25" height="20" fill="#87CEEB" stroke="#4682B4" stroke-width="1"/>
  <rect x="220" y="130" width="25" height="20" fill="#87CEEB" stroke="#4682B4" stroke-width="1"/>
  <rect x="255" y="130" width="25" height="20" fill="#87CEEB" stroke="#4682B4" stroke-width="1"/>

  <!-- Lower floor windows -->
  <rect x="120" y="180" width="25" height="20" fill="#87CEEB" stroke="#4682B4" stroke-width="1"/>
  <rect x="155" y="180" width="25" height="20" fill="#87CEEB" stroke="#4682B4" stroke-width="1"/>
  <rect x="220" y="180" width="25" height="20" fill="#87CEEB" stroke="#4682B4" stroke-width="1"/>
  <rect x="255" y="180" width="25" height="20" fill="#87CEEB" stroke="#4682B4" stroke-width="1"/>

  <!-- Main entrance -->
  <rect x="190" y="190" width="20" height="30" fill="#8B4513" stroke="#654321" stroke-width="2"/>
  <circle cx="195" cy="200" r="2" fill="#FFD700"/>
  <circle cx="205" cy="200" r="2" fill="#FFD700"/>

  <!-- Balconies -->
  <rect x="115" y="150" width="70" height="8" fill="#D2691E" stroke="#8B4513" stroke-width="1"/>
  <rect x="215" y="150" width="70" height="8" fill="#D2691E" stroke="#8B4513" stroke-width="1"/>
  <rect x="110" y="150" width="8" height="20" fill="#D2691E" stroke="#8B4513" stroke-width="1"/>
  <rect x="287" y="150" width="8" height="20" fill="#D2691E" stroke="#8B4513" stroke-width="1"/>
  <rect x="182" y="150" width="8" height="20" fill="#D2691E" stroke="#8B4513" stroke-width="1"/>
  <rect x="210" y="150" width="8" height="20" fill="#D2691E" stroke="#8B4513" stroke-width="1"/>

  <!-- Pillars -->
  <rect x="115" y="158" width="6" height="32" fill="#F5DEB3" stroke="#D2691E" stroke-width="1"/>
  <rect x="279" y="158" width="6" height="32" fill="#F5DEB3" stroke="#D2691E" stroke-width="1"/>
  <rect x="187" y="158" width="6" height="32" fill="#F5DEB3" stroke="#D2691E" stroke-width="1"/>
  <rect x="207" y="158" width="6" height="32" fill="#F5DEB3" stroke="#D2691E" stroke-width="1"/>

  <!-- Side extensions -->
  <rect x="70" y="140" width="30" height="50" fill="url(#duplex)" stroke="#D2691E" stroke-width="1"/>
  <rect x="300" y="140" width="30" height="50" fill="url(#duplex)" stroke="#D2691E" stroke-width="1"/>

  <!-- Garden elements -->
  <circle cx="50" cy="250" r="18" fill="#228B22"/>
  <circle cx="350" cy="250" r="18" fill="#228B22"/>
  <circle cx="80" cy="240" r="15" fill="#32CD32"/>
  <circle cx="320" cy="240" r="15" fill="#32CD32"/>
  <rect x="30" y="230" width="40" height="40" fill="#228B22"/>
  <rect x="330" y="230" width="40" height="40" fill="#228B22"/>

  <!-- Traditional Indian elements -->
  <circle cx="200" cy="95" r="7" fill="#FFD700" stroke="#FFA500" stroke-width="2"/>
  <path d="M185 95 Q200 85 210 95" fill="none" stroke="#FFA500" stroke-width="2"/>

  <!-- Stairs to upper floor -->
  <rect x="175" y="220" width="50" height="10" fill="#D2B48C" stroke="#8B4513" stroke-width="1"/>
  <rect x="180" y="210" width="40" height="10" fill="#D2B48C" stroke="#8B4513" stroke-width="1"/>
  <rect x="185" y="200" width="30" height="10" fill="#D2B48C" stroke="#8B4513" stroke-width="1"/>

  <!-- Title -->
  <text x="200" y="20" text-anchor="middle" fill="#8B4513" font-family="serif" font-size="16" font-weight="bold">Spacious Indian Duplex</text>
</svg>
